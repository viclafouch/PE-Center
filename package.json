{
  "name": "pe-center",
  "version": "3.1.0",
  "description": "A web extension which allows to find and copy any tips about the Google Help Center.",
  "scripts": {
    "dev": "npm run dev:chrome",
    "dev:chrome": "npx cross-env TARGET=chrome webpack --mode=development",
    "dev:firefox": "npx cross-env TARGET=firefox webpack --mode=development",
    "build": "npx cross-env TARGET=chrome webpack --mode=production",
    "publish": "node tasks/publish.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/PE-Center.git"
  },
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/PE-Center/issues"
  },
  "keywords": [
    "extensions",
    "chrome",
    "firefox",
    "react",
    "react-hooks",
    "rss-feed",
    "google"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@viclafouch/eslint-config-viclafouch": "^2.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.1.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
