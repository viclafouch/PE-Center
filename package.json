  "scripts": {
    "start": "npm run dev:chrome",
    "dev:chrome": "cross-env TARGET=chrome webpack -d --progress --hide-modules --display-error-details",
    "dev:firefox": "cross-env TARGET=firefox webpack -d --progress --hide-modules --display-error-details",
    "build": "cross-env TARGET=firefox webpack -p --progress --hide-modules && cross-env TARGET=chrome webpack -p --progress --hide-modules",
    "publish": "node tasks/publish.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
