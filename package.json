{
  "name": "pe-center",
  "version": "3.0.7",
  "description": "A web extension which allows to find and copy any tips about the Google Help Center.",
  "scripts": {
    "start": "npm run dev:chrome",
    "dev:chrome": "cross-env TARGET=chrome webpack -d --progress --hide-modules --display-error-details",
    "dev:firefox": "cross-env TARGET=firefox webpack -d --progress --hide-modules --display-error-details",
    "build": "cross-env TARGET=firefox webpack -p --progress --hide-modules && cross-env TARGET=chrome webpack -p --progress --hide-modules",
    "publish": "node tasks/publish.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/PE-Center.git"
  },
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/PE-Center/issues"
  },
  "keywords": [
    "extensions",
    "chrome",
    "firefox",
    "react",
    "react-hooks",
    "rss-feed",
    "google"
  ],
  "homepage": "https://github.com/viclafouch/PE-Center#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "i18next": "^17.0.6",
    "notistack": "^0.8.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.2",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "adm-zip": "^0.4.13",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.5",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-fix-style-only-entries": "^0.3.0"
  }
}
