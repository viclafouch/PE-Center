{
  "name": "pe-center",
  "version": "3.0.14",
  "description": "A web extension which allows to find and copy any tips about the Google Help Center.",
  "scripts": {
    "start": "npm run dev:chrome",
    "dev:chrome": "cross-env TARGET=chrome webpack -d --progress --hide-modules --display-error-details",
    "dev:firefox": "cross-env TARGET=firefox webpack -d --progress --hide-modules --display-error-details",
    "build": "cross-env TARGET=firefox webpack -p --progress --hide-modules && cross-env TARGET=chrome webpack -p --progress --hide-modules",
    "publish": "node tasks/publish.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/PE-Center.git"
  },
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/PE-Center/issues"
  },
  "keywords": [
    "extensions",
    "chrome",
    "firefox",
    "react",
    "react-hooks",
    "rss-feed",
    "google"
  ],
  "homepage": "https://github.com/viclafouch/PE-Center#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "i18next": "^17.0.18",
    "notistack": "^0.9.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-i18next": "^10.13.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@viclafouch/eslint-config-viclafouch": "^1.0.2",
    "adm-zip": "^0.4.13",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-fix-style-only-entries": "^0.4.0"
  }
}
