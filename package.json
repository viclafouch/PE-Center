{
  "name": "pe-center",
  "version": "3.1.0",
  "description": "A web extension which allows to find and copy any tips about the Google Help Center.",
  "scripts": {
    "dev": "npm run dev:chrome",
    "dev:chrome": "npx cross-env TARGET=chrome webpack --mode=development",
    "dev:firefox": "npx cross-env TARGET=firefox webpack --mode=development",
    "build": "npx cross-env TARGET=chrome webpack --mode=production",
    "publish": "node tasks/publish.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/PE-Center.git"
  },
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/PE-Center/issues"
  },
  "keywords": [
    "extensions",
    "chrome",
    "firefox",
    "react",
    "react-hooks",
    "rss-feed",
    "google"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@testing-library/react": "^11.2.2",
    "@viclafouch/eslint-config-viclafouch": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "@stores/(.*)$": "<rootDir>/src/js/stores/$1"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "babel-jest": "^26.6.3",
    "i18next": "^19.8.4",
    "immer": "^8.0.0",
    "prop-types": "^15.7.2",
    "prop-types-extra": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-test-renderer": "^17.0.1",
    "styled-components": "^5.2.1",
    "use-debounce": "^5.1.0"
  }
}
